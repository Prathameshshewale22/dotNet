@using DataLayer;
@{
    ViewData["List"]="student list";
}

<h1>Students</h1>
<div class="text-center">

    @{
       List<Student> stud=(List<Student>)ViewData["listOfStudent"];

           
                    
        <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">StudentId</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
     @foreach (Student item in stud)
            {
    <tr>
      <th scope="row">1</th>
      <td>@item.Sid</td>
      <td>@item.Name</td>
      <td>@item.Email</td>
    </tr>
    @* <tr>
      <th scope="row">2</th>
      <td>Jacob</td>
      <td>Thornton</td>
      <td>@fat</td>
    </tr>
    <tr>
      <th scope="row">3</th>
      <td>Larry</td>
      <td>the Bird</td>
      <td>@twitter</td>
    </tr> *@
     }
  </tbody>
</table>
   
    }
</div>