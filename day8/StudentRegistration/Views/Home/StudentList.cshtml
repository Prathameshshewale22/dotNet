@using DataLayer;
@{
    ViewData["List"]="student list";
}
 

<h1>Students</h1>
<div class="text-center">

    @{
      int  count=1;
       List<Student> stud=(List<Student>)ViewData["listOfStudent"];

           
                    
        <table class="table">
  <thead>
    <tr>
      <th scope="col">SrNo</th>
      <th scope="col">StudentId</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    
     @foreach (Student item in stud)
            {
    <tr>
      <th scope="row">@count</th> 
      <td>@item.Sid</td>
      <td>@item.Name</td>
      <td>@item.Email</td>
    </tr>
     count=count+1;
     }
  </tbody>
</table>
   
    }
</div>